<h1>All the students</h1>

<div class="grid-container">
  <ul style="list-style: none;">
    <div class='student-cards'>
    <% @students.each do |s| %>
      <div class='student-card'>
      <%= image_tag ("devil-mad-icon.png") %>
        <ul  style="list-style: none;">
          <li><strong><%= s.first_name%></strong>
           <%= link_to "More info", edit_missiopath(mission)    %>
          <ul style="list-style: none;">
        </ul>
      </div>
    <% end %>
    </div>
  </ul>
</div>
